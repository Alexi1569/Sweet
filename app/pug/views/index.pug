              .banner__inner
                p.banner__title Название 2
                p.banner__descr Lorem ipsum dolor sit amet
        .swiper-pagination

    section.constructor.line-section.pt.pb(data-lines="constructor" style="background-image: url(./img/constructor-bg.jpg)")
      .container
        p.caption Конструктор заказа
        .constructor__content.mt
          .constructor__col.line-wrap
            .constructor__col-title.line-parent
              p Вид товара
              <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" class="line-item" viewBox="0 0 78.69 29.2"><path d="M13.61 5.58C13 2.33 8.22 2 5.44 3.76a11.57 11.57 0 0 0-3.19 15.56 16 16 0 0 0 4.56 4.27c7.65 5.08 17.53 6.18 26.49 4.14s17.05-7 23.92-13.08C63.72 8.87 70 1.64 78.63.5" stroke="#000" stroke-miterlimit="10" stroke-dasharray="4 5" fill="none"/></svg>
            .constructor__col-content
              .form__group-inputs
                .form__group.form__group--select
                  label.form__group-label Вид товара
                  .form__group-input
                    select
                      option(selected data-img="./img/constructor-view-img.png") Пряники
                      option(data-img="./img/constructor-box-img.png") Печеньки
                      option(data-img="./img/constructor-view-img.png") Сладости
              .constructor__col-img
                img(src="./img/constructor-view-img.png" alt="alt")
          .constructor__col.line-wrap
            .constructor__col-title.line-parent
              p Оформление
              <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" class="line-item" viewBox="0 0 217.92 50.77"><path d="M.27 13C10.23 6.69 21 .17 32.77.51c8.49.25 16.46 4.1 23.57 8.74S70 19.39 77.39 23.57a69.49 69.49 0 0 0 33.22 9c4.85.06 9.82-.43 14.2-2.51s8.1-6 9-10.75a8.17 8.17 0 0 0-.12-3.87c-1.4-4.54-7.92-5.77-11.88-3.14s-5.63 7.66-6 12.4a20.27 20.27 0 0 0 1.3 9.55c2.62 6 8.94 9.57 15.06 12a53.8 53.8 0 0 0 12.89 3.69c9.68 1.31 19.57-1.34 28.29-5.73s16.47-10.5 24.14-16.54l20.12-15.87" stroke="#000" stroke-miterlimit="10" stroke-dasharray="4 5" fill="none"/></svg>
            .constructor__col-content
              .form__group-inputs
                .form__group.form__group--select
                  label.form__group-label Тематика
                  .form__group-input
                    select
                      option(selected disabled) Тематика
                      option Тематика 1
                      option Тематика 2
                .form__group.form__group--select
                  label.form__group-label Цвет
                  .form__group-input
                    select
                      option(selected disabled) Цвет
                      option Цвет 1
                      option Цвет 2
                .form__group.form__group--file
